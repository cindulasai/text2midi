# Environment variables required by the application.
# Copy this file to .env and fill in your values.
# Never commit .env to version control.

# MiniMax M2.5 API key (default provider) – obtain from https://platform.minimaxi.com/
MINIMAX_API_KEY=

# Groq API key (first fallback) – obtain from https://console.groq.com/
GROQ_API_KEY=

# Google Gemini API key (second fallback) – obtain from https://aistudio.google.com/
GEMINI_API_KEY=
